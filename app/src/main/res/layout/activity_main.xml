        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="send11"
        android:text="11"
        android:layout_marginLeft="104dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/button12" />
    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="send12"
        android:text="12"
        app:layout_constraintLeft_toRightOf="@+id/button15"
        android:layout_marginLeft="7dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button15" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="send2"
        android:text="2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/TextView2" />

    <TextView
        android:id="@+id/TextView"
        android:layout_width="107dp"
        android:layout_height="44dp"
        android:ems="10"
        android:text="Last Input:"
        android:layout_marginLeft="6dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/chart" />

    <TextView
        android:id="@+id/TextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text="None"
        app:layout_constraintBaseline_toBaselineOf="@+id/TextView"
        android:layout_marginRight="10dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/TextView"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.546" />

</android.support.constraint.ConstraintLayout>
